{
    "name": "jpcaparas/trademe-php-api",
    "description": "A PHP client for the Trade Me API",
    "type": "library",
    "license": "MIT",
    "keywords": ["trademe", "trade-me"],
    "authors": [
        {
            "name": "JP Caparas",
            "email": "jp@jpcaparas.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JPCaparas\\TradeMeAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/oauth-subscriber": "^0.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "phpspec/prophecy": "^1.8",
        "symfony/var-dumper": "^4.2",
        "php-coveralls/php-coveralls": "^2.1"
    }
}
